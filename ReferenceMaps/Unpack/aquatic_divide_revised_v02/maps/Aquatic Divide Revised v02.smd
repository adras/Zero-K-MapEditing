		SunColor=1 1 1;
		CloudDensity=0.1;

		MinWind=12;
		MaxWind=28;
	}
	[WATER]
	{
		WaterSurfaceColor=0.44 0.66 1;
		WaterPlaneColor=0.76 0.88 1;
		WaterBaseColor=0.4 0.4 1;
		WaterAbsorb=0.2 0.2 0.4;
		WaterMinColor=.4 .4 .8;
	}
	[LIGHT]
	{
		SunDir=0.4 0.4 -0.5;						//direction of sun (spring will normalize it later)(y component is upward)
		
		GroundAmbientColor=0.95 0.95 0.95;		//ambient (non sun lit) color of ground (and grass trees etc)
		GroundSunColor=0.25 0.25 0.25;			//color of ground where fully sun lit (added to ambient)
