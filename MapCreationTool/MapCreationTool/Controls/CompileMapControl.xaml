<UserControl x:Class="MapCreationTool.Controls.CompileMapControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MapCreationTool.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid DataContext="Self">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <local:CompileSetting Grid.Row="0" SettingName="Output file" SettingDescription="The output smf file to create. E.g Zero-Prime.smf" SettingIsEnabled="True" SettingHasFileBrowser="true"></local:CompileSetting>
        <local:CompileSetting Grid.Row="1" SettingName="Diffuse texture" SettingDescription="The diffuse map file to use." SettingIsEnabled="True"></local:CompileSetting>
        <local:CompileSetting Grid.Row="2" SettingName="test" SettingIsEnabled="True"></local:CompileSetting>
        <local:CompileSetting Grid.Row="3" SettingName="test" SettingIsEnabled="True"></local:CompileSetting>
        <local:CompileSetting Grid.Row="4" SettingName="test" SettingIsEnabled="True"></local:CompileSetting>
    </Grid>
</UserControl>
